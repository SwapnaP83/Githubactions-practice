name: First-Workflow
on: wokflow_dispatch
jobs:  
  first-job:
    runs_on: ubuntu_latest
    steps:
      - name: print statement
        run: echo "Hello World"

      - name: print goodbye
        run: echo "Bye for now"

       - name: Print multiple lines
         run |
           echo "Hello Tester",
           echo "Bye for now"
        

      
